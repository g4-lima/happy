{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "class-methods-use-this": "off",
      "import/prefer-default-export": "off",
      "camelcase": "off",
      "import/no-unresolved": "off",
      "import/extensions": "off",
      "no-undef": "off"
    }
}
